    <script>
      const loadFindify = setInterval(() => {
        if((findify.widgets)) {

          const itemCount = {{ cart.item_count }};
          const widget = (itemCount > 0)
            ? 'custom-findify-rec-8'
            : 'cart-findify-rec-13'

          const cartProductIdsInit = Array.from(document.querySelectorAll('[data-findify-event="update-cart"] [data-findify-item-id]')).map(element => element.getAttribute('data-findify-item-id'));
          findify.widgets.attach('#findify-cart-sidebar-recommendation', 'recommendation', {
            slot: widget,
            widgetKey: 'sidecart-rec-widget',
            ...findify.config.getIn(['features', 'recommendations', widget]).toJS()
          });

          if(cartProductIdsInit){
            findify?.widgets.get('sidecart-rec-widget').agent.defaults({
              item_ids: cartProductIdsInit
            });
          }
          console.log('findify?.widgets',findify?.widgets.list());
          clearInterval(loadFindify);
        }
      }, 500);

      const checkRecommendationText = setInterval(() => {
          const recommendationText = document.querySelector('.findify-cart-sidebar-recommendation .findify-components--text');
          if (recommendationText) {
              recommendationText.textContent = 'Recommended Products';
              recommendationText.style.display = 'block';
              recommendationText.style.textAlign = 'center';
              recommendationText.style.fontSize = '20px';
              document.querySelector('.findify-cart-sidebar-recommendation').style.display = 'block';
              clearInterval(checkRecommendationText); // Stop checking once the element is found and styled
          }

          if (window.hide_upsell) {
            const ids = [
                "7898525794543",
                "8233015509231",
                "7898508067055",
                "8672937509103",
                "8311993434351",
                "8233015312623",
                "7898533822703",
                "8070400016623",
                "8560160801007",
                "8311993499887",
                "7936352682223",
                "8311993401583"
            ];

            // Iterate over each ID
            ids.forEach(id => {
                // Escape the ID for valid CSS selector
                const escapedId = `#\\3${id.charAt(0)} ${id.slice(1)}`;
                // Select the specific anchor element within the .findify-cart-sidebar-recommendation class
                let element = document.querySelector(`.findify-cart-sidebar-recommendation a${escapedId}`);

                if (element) {
                    // Find the closest parent with the specified class and hide it
                    let parentElement = element.closest('.findify-components-common--grid__column.findify-components-common--grid__column-6');
                    if (parentElement) {
                        parentElement.style.display = 'none';
                    }
                }
            });
          }
      }, 500);

      const reloadRec = (evt) => {
        setTimeout(() => {
            const cartRecWidget = findify?.widgets.get('sidecart-rec-widget');
            let cartProductIds = Array.from(document.querySelectorAll('.cart-item-product-title')).map(element => element.getAttribute('data-productid'));

            if (cartRecWidget){
              cartRecWidget.agent.defaults({
                item_ids: cartProductIds
              });
            }
          }, 2000);
      }

      document.addEventListener('cartUpdateEvent', (evt) => {
        reloadRec(evt);
        let productsList = document.querySelectorAll('.cart-item-remove');
        productsList.forEach(item => item.addEventListener('click', () => reloadRec()));
      });
    </script>
